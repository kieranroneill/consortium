{
  "name": "consortium",
  "private": true,
  "version": "0.1.0",
  "main": "main.js",
  "description": "Yet another ethereum wallet.",
  "author": {
    "name": "Kieran O'Neill",
    "email": "kieran.oneill@element43.xyz",
    "url": "http://element43.xyz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kieranroneill/consortium"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "babel": "babel",
    "build": "webpack",
    "code-analysis": "plato -r -t consortium -d analysis main.js app/src/ -e .eslintrc",
    "clean": "rm -rf node_modules/",
    "dist": "build",
    "eslint": "eslint",
    "istanbul": "istanbul",
    "lint": "eslint main.js app/src/**/*.js* test/**/*.js*",
    "mocha": "mocha",
    "pack": "build --dir",
    "plato": "plato",
    "postinstall": "install-app-deps",
    "start": "electron . --enable-logging",
    "test": "npm run build && istanbul cover _mocha -- ./test/e2e/*.test.js ./app/src/**/*.spec.js --reporter mocha-junit-reporter",
    "test:e2e": "npm run build && mocha ./test/e2e/*.test.js",
    "test:functional": "mocha ./app/src/**/*.spec.js",
    "watch": "webpack-dev-server",
    "webpack": "webpack"
  },
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "favicons-webpack-plugin": "0.0.7",
    "html-webpack-plugin": "^2.26.0",
    "node-sass": "^4.1.1",
    "plato": "^1.7.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "electron": "^1.4.13",
    "electron-builder": "^10.17.3",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^6.8.0",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.12.2",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^1.3.1",
    "redux-devtools": "^3.3.1",
    "sinon": "^1.17.7",
    "spectron": "^3.4.1"
  },
  "build": {
    "appId": "your.id",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "iconUrl": "(windows-only) https link to icon"
    }
  },
  "directories": {
    "output": "packages"
  }
}
